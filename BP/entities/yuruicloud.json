								}
							},
							{
								"weight": 95,
								"add": {
									"component_groups": [
										"adult"
									]
								}
							}
						]
					}
				]
			},
			"minecraft:entity_transformed": {
				"sequence": [
					{
						"filters": {
							"test": "has_component",
							"subject": "other",
							"value": "minecraft:is_baby"
						},
						"add": {
							"component_groups": [
								"baby"
							]
						}
					},
