							{
								"test": "is_family",
								"subject": "other",
								"operator": "!=",
								"value": "villager"
							},
							{
								"test": "has_component",
								"subject": "self",
								"operator": "!=",
								"value": "minecraft:attack_cooldown"
							}
						]
					},
