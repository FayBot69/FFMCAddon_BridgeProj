					"on_celebration_end_event": {
						"event": "minecraft:stop_celebrating",
						"target": "self"
					}
				},
				"minecraft:behavior.move_outdoors": {
					"priority": 2,
					"speed_multiplier": 0.8,
					"timeout_cooldown": 8
				}
			},
			"baby": {
				"minecraft:is_baby": {},
