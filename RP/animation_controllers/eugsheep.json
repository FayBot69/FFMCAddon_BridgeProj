{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.eugsheep": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"animations": [
						"idle"
					],
					"transitions": [
						{
							"walking": "query.ground_speed"
						}
					]
				},
				"walking": {
					"animations": [
						"walk"
					],
					"transitions": [
						{
							"idle": "!query.ground_speed"
						}
					]
				}
			}
		}
	}
}